<?php

namespace MXZ\PCGBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Feats
 */
class Feats
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $feat;

    /**
     * @var string
     */
    private $prerequisites;

    /**
     * @var string
     */
    private $benefit;

    /**
     * @var string
     */
    private $url;

    /**
     * @var boolean
     */
    private $combat;

    /**
     * @var boolean
     */
    private $panache;

    /**
     * @var boolean
     */
    private $style;

    /**
     * @var boolean
     */
    private $metamagic;

    /**
     * @var boolean
     */
    private $teamwork;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set feat
     *
     * @param string $feat
     * @return Feats
     */
    public function setFeat($feat)
    {
        $this->feat = $feat;

        return $this;
    }

    /**
     * Get feat
     *
     * @return string 
     */
    public function getFeat()
    {
        return $this->feat;
    }

    /**
     * Set prerequisites
     *
     * @param string $prerequisites
     * @return Feats
     */
    public function setPrerequisites($prerequisites)
    {
        $this->prerequisites = $prerequisites;

        return $this;
    }

    /**
     * Get prerequisites
     *
     * @return string 
     */
    public function getPrerequisites()
    {
        return $this->prerequisites;
    }

    /**
     * Set benefit
     *
     * @param string $benefit
     * @return Feats
     */
    public function setBenefit($benefit)
    {
        $this->benefit = $benefit;

        return $this;
    }

    /**
     * Get benefit
     *
     * @return string 
     */
    public function getBenefit()
    {
        return $this->benefit;
    }

    /**
     * Set url
     *
     * @param string $url
     * @return Feats
     */
    public function setUrl($url)
    {
        $this->url = $url;

        return $this;
    }

    /**
     * Get url
     *
     * @return string 
     */
    public function getUrl()
    {
        return $this->url;
    }

    /**
     * Set combat
     *
     * @param boolean $combat
     * @return Feats
     */
    public function setCombat($combat)
    {
        $this->combat = $combat;

        return $this;
    }

    /**
     * Get combat
     *
     * @return boolean 
     */
    public function getCombat()
    {
        return $this->combat;
    }

    /**
     * Set panache
     *
     * @param boolean $panache
     * @return Feats
     */
    public function setPanache($panache)
    {
        $this->panache = $panache;

        return $this;
    }

    /**
     * Get panache
     *
     * @return boolean 
     */
    public function getPanache()
    {
        return $this->panache;
    }

    /**
     * Set style
     *
     * @param boolean $style
     * @return Feats
     */
    public function setStyle($style)
    {
        $this->style = $style;

        return $this;
    }

    /**
     * Get style
     *
     * @return boolean 
     */
    public function getStyle()
    {
        return $this->style;
    }

    /**
     * Set metamagic
     *
     * @param boolean $metamagic
     * @return Feats
     */
    public function setMetamagic($metamagic)
    {
        $this->metamagic = $metamagic;

        return $this;
    }

    /**
     * Get metamagic
     *
     * @return boolean 
     */
    public function getMetamagic()
    {
        return $this->metamagic;
    }

    /**
     * Set teamwork
     *
     * @param boolean $teamwork
     * @return Feats
     */
    public function setTeamwork($teamwork)
    {
        $this->teamwork = $teamwork;

        return $this;
    }

    /**
     * Get teamwork
     *
     * @return boolean 
     */
    public function getTeamwork()
    {
        return $this->teamwork;
    }
}
