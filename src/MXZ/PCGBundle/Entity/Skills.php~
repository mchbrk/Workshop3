<?php

namespace MXZ\PCGBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Skills
 */
class Skills
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $skill;

    /**
     * @var string
     */
    private $ability;

    /**
     * @var string
     */
    private $untrained;

    /**
     * @var boolean
     */
    private $bbn;

    /**
     * @var boolean
     */
    private $brd;

    /**
     * @var boolean
     */
    private $clr;

    /**
     * @var boolean
     */
    private $drd;

    /**
     * @var boolean
     */
    private $ftr;

    /**
     * @var boolean
     */
    private $mnk;

    /**
     * @var boolean
     */
    private $pal;

    /**
     * @var boolean
     */
    private $rgr;

    /**
     * @var boolean
     */
    private $rog;

    /**
     * @var boolean
     */
    private $sor;

    /**
     * @var boolean
     */
    private $wiz;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set skill
     *
     * @param string $skill
     * @return Skills
     */
    public function setSkill($skill)
    {
        $this->skill = $skill;

        return $this;
    }

    /**
     * Get skill
     *
     * @return string
     */
    public function getSkill()
    {
        return $this->skill;
    }

    /**
     * Set ability
     *
     * @param string $ability
     * @return Skills
     */
    public function setAbility($ability)
    {
        $this->ability = $ability;

        return $this;
    }

    /**
     * Get ability
     *
     * @return string
     */
    public function getAbility()
    {
        return $this->ability;
    }

    /**
     * Set untrained
     *
     * @param string $untrained
     * @return Skills
     */
    public function setUntrained($untrained)
    {
        $this->untrained = $untrained;

        return $this;
    }

    /**
     * Get untrained
     *
     * @return string
     */
    public function getUntrained()
    {
        return $this->untrained;
    }

    /**
     * Set bbn
     *
     * @param boolean $bbn
     * @return Skills
     */
    public function setBbn($bbn)
    {
        $this->bbn = $bbn;

        return $this;
    }

    /**
     * Get bbn
     *
     * @return boolean
     */
    public function getBbn()
    {
        return $this->bbn;
    }

    /**
     * Set brd
     *
     * @param boolean $brd
     * @return Skills
     */
    public function setBrd($brd)
    {
        $this->brd = $brd;

        return $this;
    }

    /**
     * Get brd
     *
     * @return boolean
     */
    public function getBrd()
    {
        return $this->brd;
    }

    /**
     * Set clr
     *
     * @param boolean $clr
     * @return Skills
     */
    public function setClr($clr)
    {
        $this->clr = $clr;

        return $this;
    }

    /**
     * Get clr
     *
     * @return boolean
     */
    public function getClr()
    {
        return $this->clr;
    }

    /**
     * Set drd
     *
     * @param boolean $drd
     * @return Skills
     */
    public function setDrd($drd)
    {
        $this->drd = $drd;

        return $this;
    }

    /**
     * Get drd
     *
     * @return boolean
     */
    public function getDrd()
    {
        return $this->drd;
    }

    /**
     * Set ftr
     *
     * @param boolean $ftr
     * @return Skills
     */
    public function setFtr($ftr)
    {
        $this->ftr = $ftr;

        return $this;
    }

    /**
     * Get ftr
     *
     * @return boolean
     */
    public function getFtr()
    {
        return $this->ftr;
    }

    /**
     * Set mnk
     *
     * @param boolean $mnk
     * @return Skills
     */
    public function setMnk($mnk)
    {
        $this->mnk = $mnk;

        return $this;
    }

    /**
     * Get mnk
     *
     * @return boolean
     */
    public function getMnk()
    {
        return $this->mnk;
    }

    /**
     * Set pal
     *
     * @param boolean $pal
     * @return Skills
     */
    public function setPal($pal)
    {
        $this->pal = $pal;

        return $this;
    }

    /**
     * Get pal
     *
     * @return boolean
     */
    public function getPal()
    {
        return $this->pal;
    }

    /**
     * Set rgr
     *
     * @param boolean $rgr
     * @return Skills
     */
    public function setRgr($rgr)
    {
        $this->rgr = $rgr;

        return $this;
    }

    /**
     * Get rgr
     *
     * @return boolean
     */
    public function getRgr()
    {
        return $this->rgr;
    }

    /**
     * Set rog
     *
     * @param boolean $rog
     * @return Skills
     */
    public function setRog($rog)
    {
        $this->rog = $rog;

        return $this;
    }

    /**
     * Get rog
     *
     * @return boolean
     */
    public function getRog()
    {
        return $this->rog;
    }

    /**
     * Set sor
     *
     * @param boolean $sor
     * @return Skills
     */
    public function setSor($sor)
    {
        $this->sor = $sor;

        return $this;
    }

    /**
     * Get sor
     *
     * @return boolean
     */
    public function getSor()
    {
        return $this->sor;
    }

    /**
     * Set wiz
     *
     * @param boolean $wiz
     * @return Skills
     */
    public function setWiz($wiz)
    {
        $this->wiz = $wiz;

        return $this;
    }

    /**
     * Get wiz
     *
     * @return boolean
     */
    public function getWiz()
    {
        return $this->wiz;
    }
}
